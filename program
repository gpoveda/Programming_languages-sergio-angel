    switch (menu) {
    case 1:
      login();
      menuProfesor();
      break;
    case 2:
      login();
      menuEst();
      break;
    case 3:
      exit(1);
      break;
    default:
      printf("Opcion invalida \n")
    }
  }
  void login() {
    printf("\n digite su usuario \t")
    scanf("%d", & user);
    printf("%d", & psd);
    var = validacion(user, psd)
  }
  int validacion(int usuario, int contrasena) {
    for (i = 0; i < 2; i++) {
      if (d[i].usuario == user && d[i].contrasena == psd) {
        return i;
        break
      } else {
        printf("\n Clave o usuario invalido");
        exit(1);
      }
      menuProfesor();
    }
  }
}
